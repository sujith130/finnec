# Environment Variables for Finnec Application
# Copy this file to .env and fill in your actual values

# Flask Configuration
FLASK_APP=app.py
FLASK_ENV=production
# Flask session signing (any random string, or same as OpenRouter key)
SECRET_KEY=your-flask-secret-or-openrouter-key

# Google Generative AI API Key (from https://aistudio.google.com/apikey)
GEMINI_API_KEY=your-gemini-api-key

# OpenRouter API Key (from https://openrouter.ai/keys)
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key

# Server Configuration
PORT=5000
HOST=0.0.0.0
